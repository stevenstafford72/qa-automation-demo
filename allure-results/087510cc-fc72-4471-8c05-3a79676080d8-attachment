# Test source

```ts
   1 | import { test, expect } from '@playwright/test';
   2 | import { production_URL } from '../../config/urls';
   3 | import { testUsers } from '../../config/test_users';
   4 |
   5 | // Login Flow on Production
   6 | // Testing Log in, Log in with Legacy PIN, Log in with Invalid PIN 
   7 |
   8 | test('Login Flow', async ({ page }) => {
   9 |     await page.goto(production_URL);
  10 |     await page.getByRole('textbox', { name: 'Enter your phone number' }).fill(testUsers[0].phoneNumber);
  11 |     await page.getByRole('textbox', { name: 'Enter your 4-digit PIN' }).fill(testUsers[0].pin);
  12 |     await page.getByRole('button', { name: 'Log In' }).click();
  13 |     await page.getByRole('button', { name: 'No Thanks' }).click();
  14 |     await expect(page).toHaveURL('https://app.spoofcard.com/');
  15 | });
  16 |
  17 | test('Login Flow with Legacy PIN', async ({ page }) => {
  18 |     await page.goto(production_URL);
  19 |     await page.getByRole('textbox', { name: 'Enter your phone number' }).fill(testUsers[0].phoneNumber);
  20 |     await page.getByRole('textbox', { name: 'Enter your 4-digit PIN' }).fill(testUsers[0].pin);
  21 |     await page.getByRole('button', { name: 'Log In' }).click();
  22 |     await page.getByRole('button', { name: 'No Thanks' }).click();
  23 |     await expect(page).toHaveURL('https://app.spoofcard.com/');
  24 | });
  25 |
  26 | test('Login Flow with Invalid PIN', async ({ page }) => {
  27 |     await page.goto(production_URL);
  28 |     await page.getByRole('textbox', { name: 'Enter your phone number' }).fill(testUsers[0].phoneNumber);
  29 |     await page.getByRole('textbox', { name: 'Enter your 4-digit PIN' }).fill('0000');
  30 |     await page.getByRole('button', { name: 'Log In' }).click();
> 31 |     await expect(page.getByText('Invalid PIN')).toBeVisible();
     |                                                 ^ Error: Timed out 5000ms waiting for expect(locator).toBeVisible()
  32 | });
  33 |
  34 |
  35 | test('"Log In Here" Links to Legacy Login Page', async ({ page }) => {
  36 |   await page.goto(production_URL);
